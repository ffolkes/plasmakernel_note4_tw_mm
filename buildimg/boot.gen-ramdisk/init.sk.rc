# StarKissed Extras

on early-init

# Configure SEAndroid booleans and enforcing mode
     setsebool debugfs 1

on init
    setenforce 0
    setprop ro.boot.selinux 0

on post-fs-data

on boot
    # Run sysinit
    start sysinit

# adb over network
on property:service.adb.tcp.port=5555
    stop adbd
    start adbd
on property:service.adb.tcp.port=-1
    stop adbd
    start adbd

service sysinit /sbin/busybox run-parts /system/etc/init.d
    disabled
    oneshot